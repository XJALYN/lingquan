<template name="cell">
 <view class="cell" >
 <view class="cell__left">
   <view class="cell__left__title">订单号:{{orderId}}</view>
   <view class="cell__left__date">{{createTime}}</view>
 </view>
 <view class="cell__right">
  <view class="cell__right__price">{{diffrentValue>0?"+":""}}{{diffrentValue}}</view>
  <view class="cell__right__status" wx:if="{{remainderStatus==0}}">结算中</view>
  <view class="cell__right__status" wx:if="{{remainderStatus==1}}">已转到余额</view>
  <view class="cell__right__status" wx:if="{{remainderStatus==2}}">已失效</view>
 </view>
 </view>
</template>