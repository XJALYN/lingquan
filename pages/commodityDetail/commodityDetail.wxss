@keyframes addMove{
  0%{
    bottom: 100vh;
    left:0;
    width: 750rpx;
    height: 750rpx;
    transform: translate(0,50%);
  }
  40%{
    bottom: calc(100vh - 375rpx);
    left:375rpx;
    transform: translate(0,-25rpx);
     width: 50rpx;
     height: 50rpx;
  }
