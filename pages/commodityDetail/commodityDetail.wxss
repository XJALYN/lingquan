.goIndex text{font-size: 22rpx;color: #666;text-align: center;margin: 0;padding: 0;display:block;width: 100%;height: auto;}

.goKshop{margin-top:5rpx;display: block;width:120rpx;height: 100%;float: left;position: relative;}
.goKshop icon{display: block;font-size: 38rpx;color:#666666;margin:0rpx auto 5rpx auto;text-align: center;}
.gkNum{display: block;width:48rpx;height: 34rpx;border-radius: 17rpx;background-color:#F61B6D;color: #fff;position: absolute;right:10rpx;top:-10rpx;font-size: 22rpx;text-align:center; }
.gkTxt{font-size: 22rpx;color: #666;text-align: center;margin: 0;padding: 0;display:block;width: 100%;height: auto;}
.goBuys{width:260rpx;height: 100rpx;background-color: #F61B6D;text-align: center;line-height: 100rpx;color:#F6F6F6;font-size: 30rpx;float: right; }
.goAdd{width:260rpx;height: 100rpx;background-color: #333333;text-align: center;line-height: 100rpx;color:#F6F6F6;font-size: 30rpx;float: right;}

.opThree{color: rgba(255,255,255,0.3) !important;}
.noHint{width: 100%;height: 80rpx;background-color: #666;text-align: center;line-height: 80rpx;color: #fff;font-size: 28rpx;}
.noMes{position: fixed;bottom: 0;height:180rpx;left: 0;right: 0; }


