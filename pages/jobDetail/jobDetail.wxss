/* inPages/findGoods/index.wxss */
@import "../../app.wxss";
@import "./footer/index";
@import "./commentList/index";
page{
  background: white !important;
}
.findGoods {
  margin-top: 20px;
  padding: 0rpx 30rpx;
  height: auto;
  overflow: hidden;
  margin-bottom: 50rpx;
}

.fgList {
  
  background-color: #fff;
  border-radius: 10rpx;
  margin-bottom: 17rpx;
}

.fgName {
  width: 100%;
  height: auto;
  overflow: hidden;
  position: relative;
}


.fgnHead {
  display: block;
  width: 70rpx;
  height: 70rpx;
  float: left;
  border-radius: 35rpx;
}

.fgInfo {
  width: 300rx;
  height: auto;
  overflow: hidden;
  float: left;
  margin-left: 16rpx;
}

.fgiName {
  width: 100%;
display: flex;
align-items: center;
}

.fgiName text:nth-child(1) {
  color: #343434;
  font-size: 28rpx;
  margin-right: 10rpx;
}
.fgiName__icon{
  width: 24rpx;
  height: 24rpx;
}

