.cell{
  margin-top: 20rpx;
 height: 205rpx;
 margin-left: 32rpx;
 margin-right: 32rpx;
 width: calc(100% - 64rpx);
 
 display: flex;
 position: relative;
 
 
}
.cell__bg_1{
 
  background:url(https://kshop-pro-data.oss-cn-hangzhou.aliyuncs.com/kmh/my_coupon_active.png);
  background-size: 100% 100%;
 background-repeat: no-repeat;
}
.cell__bg_2{
  background:url(https://kshop-pro-data.oss-cn-hangzhou.aliyuncs.com/kmh/my_coupon_invalid.png);
  background-size: 100% 100%;
 background-repeat: no-repeat;
}
.cell__left{
 width: 242rpx;
 height: 100%;
 
 display: flex;
 flex-direction: column;
 align-items: center;
}

.cell__left_1{
  width: 242rpx;
 height: 205rpx;

 display: flex;
 flex-direction: column;
 align-items: center;
 text-align: center;
 line-height: 205rpx;
 font-size: 50rpx;
 color: white;
}
.cell__left--invalue{
  
}
.cell__left__top{
  margin-top: 56rpx;
  height: 48rpx;
  line-height: 48rpx;
  display: flex;
  align-items: flex-end;
  color: white;
}
.cell__left__discount{
 font-size: 70rpx;

}
.cell__left__text{
 font-size: 40rpx;
 line-height: 36rpx;
 height: 36rpx;

}
.cell__left__tip{
 margin-top: 32rpx;
 height: 24rpx;
 line-height: 24rpx;
 font-size: 24rpx;
 color: white;
}
.cell__right{
  margin-left: 30rpx;
  color: #333333;
  font-size: 32rpx;
}
.cell__right__title{
  margin-top: 30rpx;
  color: #333333;
  height: 45rpx;
  font-weight: 500;
  line-height: 45rpx;
}

.cell__right__title--invalid{
  color: #999999;
}

.cell__right__date{
  margin-top: 64rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #999999;
  font-size: 22rpx;
}
.cell__right__btn-use{
 position: absolute;
 right: 30rpx;
 bottom: 23rpx;
 width: 130rpx;
 height: 54rpx;
 text-align: center;
 line-height: 54rpx;
 background-color: #F7498A;
 font-size: 24rpx;
 border-radius: 27rpx;
 color: white;
}
.cell__right__btn-use-disabled{
  background-color: #999999;
}