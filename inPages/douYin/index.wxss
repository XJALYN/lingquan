/* inPages/douYin/index.wxss */
@import "../../app.wxss";
.douyin{padding: 30rpx 32rpx;height: auto;overflow:hidden;}
.douyinBanner{width: 100%;height:260rpx;border-radius:10rpx;display: block;}
.dyList{width:100%;height: auto;overflow: hidden;padding-top:20rpx; }
.dylLi{width:334rpx;height: auto;overflow: hidden;float: left;margin-right: 18rpx;margin-bottom:20rpx;background-color: #fff; padding-bottom: 20rpx;border-radius:10rpx;}
.dyList .dylLi:nth-child(2n){margin-right: 0;}
.dyLimg{display: block;width: 100%;height:340rpx;border-radius:10rpx 10rpx 0 0;}
.dllName{padding: 20rpx 14rpx;font-size: 26rpx;color:#343434; }
.dllIco{display: block;width:52rpx;height: 29rpx;position: absolute;left: 0;top: 5rpx;border-radius:4rpx; }
.dllNmes{width: 100%;height:73rpx;text-indent:56rpx;position: relative;}
.dllPrice{padding: 0rpx 14rpx;height: auto;overflow: hidden; }
.dllPrice icon{display: block;width:27rpx;height: 27rpx;background-color: #E46C94;float: left;margin-right:11rpx;border-radius:4rpx;margin-top: 10rpx;}
.dllpMes{float: left;font-size: 28rpx;color:#F61B6D;display: inline-block;margin-right:15rpx; }
.dllpMes text:nth-child(1){float: left;font-size: 24rpx;color:#F61B6D;display: block;padding-top:8rpx;}
.dllpMes text:nth-child(2){float: left;font-size: 28rpx;color:#F61B6D;display: block;padding-top:5rpx;}
.dllYprice{float: left;font-size: 20rpx;color:#999999;padding-top: 11rpx;text-decoration:line-through;}

.goods-list__list__item__bottom__icon{
 width: 20rpx;
 height: 20rpx;
 background-color: #eee;
   margin:15rpx 10rpx 0 0;
   float: left
}
.goods-list__list__item__bottom__big__delete{
  width: 46rpx;
  height: 46rpx;
  margin-right:0rpx;
  float: right
}







